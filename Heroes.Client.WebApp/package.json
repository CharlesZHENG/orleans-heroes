{
  "name": "HeroesWebApp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "dotnet run",
    "start-clean-dev": "npm run clean && npm run build && npm start",
    "start-prod": "dotnet run -p Heroes.Client.WebApp.csproj --launch-profile \"App-Prod\"",
    "start-clean-prod": "npm run clean && npm run build:rel && npm run start-prod",
    "test": "karma start ClientApp/test/karma.conf.js",
    "build:vendor": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js",
    "build:app": "node node_modules/webpack/bin/webpack.js",
    "build": "npm run build:vendor && npm run build:app",
    "build:vendor:rel": "npm run build:vendor -- --env.prod",
    "build:app:rel": "npm run build:app -- --env.prod",
    "build:rel": "npm run build:vendor:rel && npm run build:app:rel",
    "clean": "rimraf ./ClientApp/dist wwwroot/dist",
    "schema:download": "apollo-codegen introspect-schema http://localhost:62552/graphql --output ClientApp/app/graphql/schema.json",
    "schema:generate": "apollo-codegen generate **/*.graphql --schema ClientApp/app/graphql/schema.json --target typescript --output ClientApp/app/graphql/schema.ts"
  },
  "dependencies": {
    "@angular-redux/form": "^6.6.0",
    "@angular-redux/router": "^6.3.1",
    "@angular-redux/store": "^6.5.7",
    "@angular/animations": "4.4.4",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/compiler-cli": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/platform-server": "4.4.4",
    "@angular/router": "4.4.4",
    "@ngtools/webpack": "1.7.2",
    "@types/webpack-env": "1.13.1",
    "angular2-template-loader": "0.6.2",
    "apollo-angular": "^0.13.1",
    "apollo-client": "^1.9.3",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.1",
    "awesome-typescript-loader": "3.2.3",
    "bootstrap": "3.3.7",
    "css": "2.2.1",
    "css-loader": "0.28.7",
    "es6-shim": "0.35.3",
    "event-source-polyfill": "0.0.9",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "1.1.4",
    "graphql-tag": "^2.4.2",
    "html-loader": "0.5.1",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.2.1",
    "json-loader": "0.5.7",
    "lodash": "^4.17.4",
    "preboot": "5.0.0",
    "raw-loader": "0.5.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.16.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "style-loader": "0.18.2",
    "to-string-loader": "1.1.5",
    "typescript": "2.5.3",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-merge": "4.1.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.23",
    "@types/chai": "4.0.4",
